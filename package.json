{
  "name": "noice-adventures",
  "version": "1.0.0",
  "description": "Small arcade game which was made in the course of a school project. In corporation with @alexwe1ss.",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "postinstall": "electron-builder install-app-deps",
    "build:all": "electron-builder -mwl --x64",
    "build:win": "electron-builder -w --publish=never",
